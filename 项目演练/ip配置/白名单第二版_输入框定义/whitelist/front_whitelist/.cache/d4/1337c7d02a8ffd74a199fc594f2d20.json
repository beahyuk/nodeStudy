r\n    };\r\n  },\r\n  methods: {\r\n    handleCancel() {\r\n      this.$emit(\"handleCancel\");\r\n    },\r\n\r\n    handleSubmit() {\r\n      this.form.validateFields((err, values) => {\r\n        if (!err) {\r\n          this.whiteIps = values.whiteIP;\r\n          console.log(`whiteIps:${this.whiteIps}`, typeof this.whiteIps);\r\n          this.submit();\r\n          // 在index首页显示输入的白名单\r\n          // this.$emit('submit')\r\n        }\r\n      });\r\n    },\r\n     submit() {\r\n        // debugger;\r\n        this.axios\r\n          .post(\"http://172.25.73.71:3000/getLatestVerInfo\", {\r\n            whiteList: this.whiteIps,\r\n          })\r\n          .then(res=>{\r\n            console.log(res);\r\n          })\r\n        this.handleCancel();\r\n        this.$message.success(\"操作成功\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>"},"lineCount":null}},"error":null,"hash":"e6fedd44c48ceccdbf33255c0fdaafbd","cacheData":{"env":{}}}